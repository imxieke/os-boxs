# font file format: 
# Desktop format:
# ttf otf ttc
# Web format:
# woff woff2 svg eot


if [[ "$(uname -s)" == 'Darwin' ]]; then
    _font_dirs=(
        ~/Library/Fonts
        /Library/Fonts
        /System/Library/Fonts
    )
fi