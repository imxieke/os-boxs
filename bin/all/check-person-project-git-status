 # @FilePath: /dockenv/Users/imxieke/.boxs/bin/all/check-person-project-git-status
### 

PROJECT_LISTS=$(cat ~/.boxs/etc/person-project.txt)
[ ! -f "${PROJECT_LISTS}" ] && touch $PROJECT_LISTS

if [[ -n "$PROJECT_LISTS" ]]; then
    echo $$PROJECT_LISTS
fi